{
    "root": true,
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "prettier",
        "plugin:unicorn/recommended",
        "plugin:import/recommended",
        "plugin:promise/recommended"
    ],
    "rules": {
        "semi": 2,
        "unicorn/prefer-module": "off",
        "import/no-extraneous-dependencies": "off",
        "unicorn/prefer-node-protocol": "off"
    }
}
